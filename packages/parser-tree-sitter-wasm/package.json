{
  "name": "@semadiff/parser-tree-sitter-wasm",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && SEMADIFF_WASM_OUT=packages/parser-tree-sitter-wasm/dist/semadiff-wasm bun ../../scripts/copy-wasm.mjs",
    "prepack": "bun ../../scripts/verify-wasm-assets.mjs --dir packages/parser-tree-sitter-wasm/dist/semadiff-wasm"
  },
  "dependencies": {
    "@semadiff/parsers": "workspace:*",
    "effect": "^4.0.0-beta.0",
    "web-tree-sitter": "^0.26.3"
  },
  "devDependencies": {
    "@tree-sitter-grammars/tree-sitter-markdown": "^0.3.2",
    "@tree-sitter-grammars/tree-sitter-yaml": "^0.7.1",
    "tree-sitter-css": "^0.25.0",
    "tree-sitter-javascript": "^0.25.0",
    "tree-sitter-json": "^0.24.8",
    "tree-sitter-toml": "^0.5.1",
    "tree-sitter-typescript": "^0.23.2"
  }
}
